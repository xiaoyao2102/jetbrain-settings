<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Debug">
        <Item>
          <IdeaActionID>ActivateDebugToolWindow</IdeaActionID>
          <ShortCut>'Alt+5'</ShortCut>
          <Description>Debug</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Directory Path">
        <Item>
          <IdeaActionID>ShowFilePath</IdeaActionID>
          <ShortCut>'Ctrl+Alt+F12'</ShortCut>
          <Description>Directory Path</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Implementation(s)">
        <Item>
          <IdeaActionID>GotoImplementation</IdeaActionID>
          <ShortCut>'Ctrl+Alt+B'</ShortCut>
          <Description>Implementation(s)</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'DubboServiceStart'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'Ctrl+F5'</ShortCut>
          <Description>Rerun 'DubboServiceStart'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'Ctrl+F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>